Welcome to <%=@user.name%>'s wardrobe.
<br />
<% if @user.dilemmas.all.empty? %>
  <%=@user.name%>'s wardrobe is empty :(
<% else %>
  <% @user.dilemmas.all.each do |dilemma|%>
    <%= image_tag(dilemma.option1.url(:medium), :class => "img-rounded well") %>
    <%= image_tag(dilemma.option2.url(:medium), :class => "img-rounded well") %>
    <br />
    <%= dilemma.occasion %>
    <%= link_to "Delete", dilemma_path(dilemma), method: :delete %>
  <% end %>
<% end %>
